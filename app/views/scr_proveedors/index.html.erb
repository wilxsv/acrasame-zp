<h1>Listing scr_proveedors</h1>

<table>
  <thead>
    <tr>
      <th>Proveedornombre</th>
      <th>Proveedordescripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scr_proveedors.each do |scr_proveedor| %>
      <tr>
        <td><%= scr_proveedor.proveedorNombre %></td>
        <td><%= scr_proveedor.proveedorDescripcion %></td>
        <td><%= link_to 'Show', scr_proveedor %></td>
        <td><%= link_to 'Edit', edit_scr_proveedor_path(scr_proveedor) %></td>
        <td><%= link_to 'Destroy', scr_proveedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

