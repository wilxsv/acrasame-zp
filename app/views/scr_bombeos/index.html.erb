<div class="module">
 <div class="module-head"><h3>Listado de bombeos</h3></div>
 <div class="module-body table">
  <table cellpadding="0" cellspacing="0" border="0" class="datatable-2 table table-bordered table-striped	 display" width="100%">
   <thead>
    <tr>
      <th>Fecha</th>
      <th>Bombeo inicio</th>
      <th>Bombeo fin</th>
      <th>Voltaje</th>
      <th>Amperaje</th>
      <th>Presion</th>
      <th>Lectura</th>
      <th>Produccion</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @scr_bombeos.each do |scr_bombeo| %>
      <tr>
        <td><%= scr_bombeo.fecha %></td>
        <td><%= scr_bombeo.bombeo_inicio.strftime("%I:%M") %></td>
        <td><%= scr_bombeo.bombeo_fin.strftime("%I:%M") %></td>
        <td><%= scr_bombeo.voltaje %></td>
        <td><%= scr_bombeo.amperaje %></td>
        <td><%= scr_bombeo.presion %></td>
        <td><%= scr_bombeo.lectura %></td>
        <td><%= scr_bombeo.produccion %></td>
        <td><%= link_to 'Ver', scr_bombeo %></td>
        <td><%= link_to 'Editar', edit_scr_bombeo_path(scr_bombeo) %></td>
        <td><%= link_to 'Eliminar', scr_bombeo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
   <tfoot>
    <tr>
      <th>Fecha</th>
      <th>Bombeo inicio</th>
      <th>Bombeo fin</th>
      <th>Voltaje</th>
      <th>Amperaje</th>
      <th>Presion</th>
      <th>Lectura</th>
      <th>Produccion</th>
      <th colspan="3"></th>
    </tr>
  </tfoot>
</table>

<br>

<%= link_to 'Nuevo bombeo', new_scr_bombeo_path, class:"btn btn-mini btn-inverse" %>
<%= render :partial => '/layouts/main/tablas' %>
