<%
# in application.js
#//= require bootstrap-datetimepicker
#//= require bootstrap-datetimepicker/core
#//= require bootstrap-datetimepicker/pickers
#//= require bootstrap-datetimepicker/locales/bootstrap-datetimepicker.es.js

%>
<!DOCTYPE html>
<html lang="es">
 <head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Registro contable :: Asociaci√≥n Rural Zapote Platanares ::</title>
  <script src="<%= javascript_url "main/jquery-1.9.1.min.js" %>" type="text/javascript"></script>
  <script src="<%= javascript_url "main/jquery-ui-1.10.1.custom.min.js" %>" type="text/javascript"></script>
  <link type="text/css" rel="stylesheet" href="<%= stylesheet_url "main/bootstrap/css/bootstrap.min.css" %>" >
  <link type="text/css" rel="stylesheet" href="<%= stylesheet_url "main/bootstrap/css/bootstrap-responsive.min.css" %>" >
  <link type="text/css" rel="stylesheet" href="<%= stylesheet_url "main/icons/css/font-awesome.css" %>" >
  <link type="text/css" rel="stylesheet" href="<%= stylesheet_url "main/theme.css" %>" >
  <%= csrf_meta_tags %>
  <%= yield :head %>
  <% if request.fullpath.match(/index/) %>
  <% else %>
   <!-- <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %> -->
   <%= javascript_include_tag 'application', 'data-turbolinks-track' => true
    %>
  <% end %>
  <%= stylesheet_link_tag :"redmond/jquery-ui.css" %>
  <script src="<%= javascript_url "jquery-ui" %>"></script>
  <%= render :partial => '/layouts/main/js' %>
 </head>
 <body>
  <% if session['rol'] == 'Directiva' %>
   <%= render :partial => '/layouts/main/head' %>
   <div class="wrapper">
    <div class="container">
     <div class="row">
      <%= render :partial => '/layouts/main/menu' %>
  <% elsif session['rol'] == 'Administracion' %>
   <%= render :partial => '/layouts/main/heada' %>
   <div class="wrapper">
    <div class="container">
     <div class="row">
      <%= render :partial => '/layouts/main/menua' %>
  <% elsif session['rol'] == 'Contabilidad' %>
   <%= render :partial => '/layouts/main/headb' %>
   <div class="wrapper">
    <div class="container">
     <div class="row">
      <%= render :partial => '/layouts/main/menub' %>
  <% elsif session['rol'] == 'Tecnico' %>
   <%= render :partial => '/layouts/main/headc' %>
   <div class="wrapper">
    <div class="container">
     <div class="row">
      <%= render :partial => '/layouts/main/menuc' %>
  <% elsif session['rol'] == 'root' %>
   <%= render :partial => '/layouts/main/headd' %>
   <div class="wrapper">
    <div class="container">
     <div class="row">
      <%= render :partial => '/layouts/main/menud' %>
  <% else %>
   <%= render :partial => '/layouts/main/headinvitado' %>
   <div class="wrapper">
    <div class="container">
     <div class="row">
      <%= render :partial => '/layouts/main/menuinvitado' %>
  <% end %>
      <div class="span9">
       <div class="content">
        <%= yield %>
       </div>
       <!--/.content-->
      </div>
     <!--/.span9-->
    </div>
   </div>
   <!--/.container-->
  </div>    
  <%= render :partial => '/layouts/main/footer' %>
 </body>
</html>
