<div class="module">
 <div class="module-head"><h3>Catalogo de beneficiario</h3></div>
 <div class="module-body table">
  <table cellpadding="0" cellspacing="0" border="0" class="datatable-2 table table-bordered table-striped	 display" width="100%">
   <thead>
    <tr>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Telefono</th>
      <th>Celular</th>
      <th>Dui</th>
      <th>ISSS</th>
      <th>Cargo</th>
      <th>Correo</th>
      <th colspan="3">Operaciones</th>
    </tr>
  </thead>
  <tbody>
    <% @scr_empleados.each do |scr_empleado| %>
      <tr>
        <td><%= scr_empleado.empleadoNombre %></td>
        <td><%= scr_empleado.empleadoApellido %></td>
        <td><%= scr_empleado.empleadoTelefono %></td>
        <td><%= scr_empleado.empleadoCelular %></td>
        <td><%= scr_empleado.empleadoDui %></td>
        <td><%= scr_empleado.empleadoIsss %></td>
        <td><%= scr_empleado.cargo_id %></td>
        <td><%= scr_empleado.empleadoEmail %></td>
        <td><%= link_to 'ver', scr_empleado %></td>
        <td><%= link_to 'Editar', edit_scr_empleado_path(scr_empleado) %></td>
        <td><%= link_to 'Destruir', scr_empleado, method: :delete, data: { confirm: 'Confirmar operacion?' } %></td>
      </tr>
    <% end %>
  </tbody>
  <tfoot>
   <tr>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Telefono</th>
      <th>Celular</th>
      <th>Dui</th>
      <th>ISSS</th>
      <th>Cargo</th>
      <th>Correo</th>
      <th colspan="3">Operaciones</th>
   </tr>
  </tfoot>
</table>
<br>
<%= link_to 'Ingresar empleado', new_scr_empleado_path, class:"btn btn-inverse" %>
 </div>
</div>
<%= render :partial => '/layouts/main/tablas' %>
