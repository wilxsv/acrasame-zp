<div class="module">
 <div class="module-head"><h3>AÃ±o Fiscal</h3></div>
 <div class="module-body table">
  <table cellpadding="0" cellspacing="0" border="0" class="datatable-2 table table-bordered table-striped	 display" width="100%">
   <thead>
    <tr>
      <th>Inicia</th>
      <th>Finaliza</th>
      <th>Habilitado</th>
      <th>Moneda</th>
      <th>Pagos por mes</th>
      <th>Organizacion</th>
      <th>Empleado</th>
      <th colspan="3">Operaciones</th>
    </tr>
  </thead>

  <tbody>
    <% @scr_det_contables.each do |scr_det_contable| %>
      <tr>
        <td><%= scr_det_contable.dConIniPeriodo %></td>
        <td><%= scr_det_contable.dConFinPeriodo %></td>
        <td><%= scr_det_contable.dConActivo %></td>
        <td><%= scr_det_contable.dConSimboloMoneda %></td>
        <td><%= scr_det_contable.dConPagoXMes %></td>
        <td>
         <% c = ScrOrganizacion.find(scr_det_contable.organizacion_id) %>
         <%= c.organizacionNombre %>
        </td>
        <td><%= link_to 'Ver', scr_det_contable %></td>
        <td><%= link_to 'Editar', edit_scr_det_contable_path(scr_det_contable) %></td>
        <td><%= link_to 'Eliminar', scr_det_contable, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
  <tfoot>
    <tr>
      <th>Inicia</th>
      <th>Finaliza</th>
      <th>Habilitado</th>
      <th>Moneda</th>
      <th>Pagos por mes</th>
      <th>Organizacion</th>
      <th>Empleado</th>
      <th colspan="3">Operaciones</th>
    </tr>
  </tfoot>
 </table>
 <br>
<%= link_to 'Nuevo periodo contable', new_scr_det_contable_path, class:"btn btn-inverse" %>
 </div>
</div>
<%= render :partial => '/layouts/main/tablas' %>
